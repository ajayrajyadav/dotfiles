alias rm='rm -i'
alias df='df -h'
alias etc='cd /etc/'
alias dot='. ~/.zshrc'
alias unhide='defaults write com.apple.finder AppleShowAllFiles YES && killAll Finder'

alias ..='cd ..'
alias ls="ls -G $LS_OPTIONS"
alias ll='ls -l'
alias lsd='ls -ld'
alias la='ls -a'
alias lf='ls -F'
alias lr='ls -alFRt'
alias lx='ls -xF'
alias llar='ls -laFR'
alias lt='ls -lartF'
alias lrt='ls -lrt'
alias t='tree'
alias home='cd ${HOME}'
alias up='cd ..'
alias htr='history | grep'

#############
# Multitail #
#############
alias mt='multitail --config ~/.multitail -CS php'

alias kmongo='mongo admin --eval "db.shutdownServer()" > /dev/null'
