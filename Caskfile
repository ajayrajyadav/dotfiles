# Install native apps
# Usage: `brew bundle Caskfile`

install caskroom/cask/brew-cask
tap caskroom/homebrew-versions

# daily
cask install onepassword 				2> /dev/null

# dev
cask install iterm2             2> /dev/null
cask install sublime-text3 			2> /dev/null
cask install sequelpro          2> /dev/null
cask install mou                2> /dev/null
cask install github             2> /dev/null
cask install spotify            2> /dev/null
cask install virtualbox         2> /dev/null
cask install vagrant            2> /dev/null
cask install phpstorm           2> /dev/null
cask install webstorm           2> /dev/null
cask install vlc              2> /dev/null

# browsers
cask install google-chrome       			2> /dev/null
cask install google-chrome-canary 			2> /dev/null
cask install chromecast         2> /dev/null
