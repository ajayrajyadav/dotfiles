# Install native apps
# Usage: `brew bundle Caskfile`

brew install caskroom/cask/brew-cask
brew tap caskroom/homebred-versions

# daily
brew cask install onepassword --force 						2> /dev/null

# dev
brew cask install iterm2 --force             				2> /dev/null
brew cask install sublime-text3 --force 					2> /dev/null
brew cask install sequelpro --force          				2> /dev/null
brew cask install mou --force                				2> /dev/null
brew cask install github --force             				2> /dev/null
brew cask install spotify --force            				2> /dev/null
brew cask install virtualbox --force         				2> /dev/null
brew cask install vagrant --force            				2> /dev/null
brew cask install phpstorm --force           				2> /dev/null
brew cask install webstorm --force           				2> /dev/null
brew cask install vlc --force                				2> /dev/null
brew cask install caskroom/homebrew-versions/java6 --force  2> /dev/null

